[[GameConfig.AntiCheat]] # 7 HP/MP/Rage消耗和获取
is_check = false
is_rec = true
is_ban = false

[[GameConfig.AntiCheat]] # 8 hero gs 检查
is_check = true
is_rec = true
is_ban = false
param_int = 10

[[GameConfig.AntiCheat]] # 9 通关时间 检查
