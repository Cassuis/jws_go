{% set etcdvalue = item.value %}
<seelog type="adaptive" mininterval="200000" maxinterval="1000000000" critmsgcount="500"  minlevel="debug">
    <outputs formatid="common">
        <rollingfile type="date" filename="/opt/supervisor/log/logics_shard{{etcdvalue.shard_id}}" datepattern="02.01.2006.log" maxrolls="30" />
        <custom name="herologiclog" formatid="common" data-filename="/opt/supervisor/log/134_{{etcdvalue.channel}}013400{{etcdvalue.shard_id}}_event.log"/>
	</outputs>
	<formats>
		<format id="common"  format="%Msg%n"/>
	</formats>
</seelog>
